{
  "name": "ai-resume",
  "version": "0.1.0",
  "private": true,
  "description": "An AI-powered resume assistant built with Next.js, OpenAI AgentSDK, and modern web technologies",
  "author": "Daniel McCarthy",
  "repository": {
    "type": "git",
    "url": "https://github.com/MacAttak/ai-resume.git"
  },
  "bugs": {
    "url": "https://github.com/MacAttak/ai-resume/issues"
  },
  "homepage": "https://github.com/MacAttak/ai-resume#readme",
  "keywords": [
    "ai",
    "resume",
    "chatbot",
    "nextjs",
    "openai",
    "typescript",
    "clerk",
    "upstash"
  ],
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:all": "npm run test && npm run test:e2e",
    "verify:vector-store": "tsx scripts/verify-vector-store.ts",
    "verify:honeyhive": "tsx scripts/evaluate-agent.ts",
    "env:pull": "vercel env pull .env.local",
    "env:pull:preview": "vercel env pull --environment=preview .env.preview.local",
    "env:pull:production": "vercel env pull --environment=production .env.production.local"
  },
  "dependencies": {
    "@ai-sdk/openai": "^3.0.26",
    "@calcom/embed-react": "^1.5.3",
    "@clerk/nextjs": "^6.37.3",
    "@openai/agents": "^0.4.6",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/sdk-trace-node": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.38.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@upstash/ratelimit": "^2.0.7",
    "@upstash/redis": "^1.35.6",
    "@vercel/kv": "^3.0.0",
    "@vercel/otel": "^2.1.0",
    "@vercel/speed-insights": "^1.2.0",
    "ai": "^6.0.77",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.3",
    "honeyhive": "^1.0.42",
    "next": "^16.0.3",
    "next-themes": "^0.4.6",
    "openai": "^6.9.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "streamdown": "^2.1.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@clerk/testing": "^1.13.15",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^25.2.2",
    "@types/react": "^19.2.6",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/ui": "^4.0.10",
    "eslint": "^10.0.0",
    "eslint-config-next": "^16.1.6",
    "jsdom": "^28.0.0",
    "lucide-react": "^0.563.0",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5",
    "vitest": "^4.0.10"
  }
}
